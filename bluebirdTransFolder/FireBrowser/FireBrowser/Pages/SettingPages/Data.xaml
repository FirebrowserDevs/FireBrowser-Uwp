<Page
    x:Class="FireBrowser.Pages.SettingPages.Data"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:FireBrowser.Pages.SettingPages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:svsettctrl="using:SVSettingsControl"
    mc:Ignorable="d">

    <StackPanel>
        <svsettctrl:SettingsBlockControl Title="Browser data" Description="">
            <svsettctrl:SettingsBlockControl.Icon>
                <FontIcon FontFamily="Segoe Fluent Icons" Glyph="&#xE13D;" />
            </svsettctrl:SettingsBlockControl.Icon>
            <Button Click="ClearBrowserData_Click" Content="Clear" />
        </svsettctrl:SettingsBlockControl>
    </StackPanel>
</Page>
